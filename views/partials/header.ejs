
<h1>Milk Crate Albums</h1>
<% if (user) { %>
<h2> <%= user.name %>'s Crate.</h2>
<form method="POST" action="/sessions/logoff">
  <input type="hidden" name="_csrf" value="<%= _csrf %>">
  <button>LOG OFF</button>
</form>
<% } %> <% if (errors) { errors.forEach((err) => { %>
<div>Error: <%= err %></div>
<% }) } %> <% if (info) { info.forEach((msg) => { %>
<div>Info: <%= msg %></div>
<% }) } %>
<hr />

